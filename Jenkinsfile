        agent any
                stages{
                      stage('Build'){
                            steps {
                                  echo "Build Stage"
                                  }
                                  }
                                  stage('Test'){
                            steps {
                                  echo "Testing Stage"
                                  }
                                  }
                                  stage('Deploy'){
                            steps {
                                  echo "Deploy Phase"
                                  }
                                 }
                                }
                              }
                                
